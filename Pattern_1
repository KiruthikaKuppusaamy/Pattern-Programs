---------------------------------------------------------------------------------------------
C Programming
---------------------------------------------------------------------------------------------

#include <stdio.h>
void main() 
{
   for(int i=1;i<=4;i++)
   {
       for(int j=1;j<=4;j++)
       {
           printf("*");
       }
       printf("\n");
   }
}


---------------------------------------------------------------------------------------------
Python Programming
---------------------------------------------------------------------------------------------

for i in range(0,5):
    for j in range(0,5):
        print("*",end=" ")
    print()


---------------------------------------------------------------------------------------------
Java Programming
---------------------------------------------------------------------------------------------

public class Main
{
 public static void main (String arg [])
 {
 for (int i=1; i<=4; i++)
   {
       for (int j=1; j<=4; j++)
       {
           System.out.print("*");
       }
       System.out.println();
   }
 }
}
